{
  "expo": {
    "name": "Founders Cube",
    "slug": "founders-cube",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "founderscube",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#F6F7F7"
    },
    "notification": {
      "icon": "./assets/icon.png",
      "color": "#000000"
    },
    "ios": {
      "supportsTablet": false,
      "requireFullScreen": true,
      "bundleIdentifier": "com.nuhaverse.founderscube",
      "associatedDomains": [
        "applinks:community.thefounderscube.com"
      ],
      "entitlements": {
        "aps-environment": "production"
      },
      "infoPlist": {
        "UISupportedInterfaceOrientations": [
          "UIInterfaceOrientationPortrait"
        ],
        "UIRequiresFullScreen": true,
        "NSPhotoLibraryUsageDescription": "We use your photo library to allow you to upload images for personalized profiles and posting blogs. For example, you can select a photo to set as your profile picture or upload a photo to your blog.",
        "NSContactsUsageDescription": "We use your contacts to let you save members' details to your device's contacts for easier access and communication.",
        "NSUserNotificationUsageDescription": "We use notifications to send you updates about new blog posts, welcome messages, and reminders for your subscriptions."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#F6F7F7"
      },
      "package": "com.nuhaverse.founderscube",
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_CONTACTS",
        "android.permission.POST_NOTIFICATIONS"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "runtimeVersion": "1.0.0",
    "extra": {
      "eas": {
        "projectId": "0f782880-8da4-4767-9343-427045cda97e"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/0f782880-8da4-4767-9343-427045cda97e"
    }
  }
}
